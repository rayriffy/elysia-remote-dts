{
  "name": "elysia-remote-dts",
  "version": "0.0.2",
  "author": "Phumrapee Limpianchop <mail@rayriffy.com>",
  "description": "A plugin that provide .d.ts types remotely for Eden Treaty to consume.",
  "license": "MIT",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "url": "git+https://github.com/rayriffy/elysia-remote-dts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": ["dist"],
  "scripts": {
    "preinstall": "npx only-allow bun",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "bun run build --watch",
    "build": "tsup",
    "lint": "biome check",
    "format": "biome check --write"
  },
  "keywords": [],
  "dependencies": {
    "debug": "^4.4.0",
    "get-tsconfig": "^4.10.0",
    "oxc-transform": "^0.67.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "2.29.2",
    "@tsconfig/node22": "22.0.1",
    "@types/bun": "^1.2.11",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.15.3",
    "elysia": "^1.2.25",
    "tsup": "8.4.0",
    "typescript": "^5.8.3",
    "vitest": "3.1.2"
  }
}
